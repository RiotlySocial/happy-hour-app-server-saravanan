{
  "name": "app",
  "version": "0.1.0",
  "description": "Riotly Happy Hour App",
  "author": "Saravanan Shanmugam",
  "license": "MIT",
  "scripts": {
    "format": "prettier --write \"src/**/*.ts\"",
    "start": "ts-node -r dotenv/config  src/main.ts",
    "start:dev": "nodemon",
    "start:debug": "npm run start:mongo && nodemon --config nodemon-debug.json",
    "start:mongo": "mkdir -p mongo/db && lsof -i TCP:27017 | grep LISTEN | awk '{print $2}' | xargs kill -9 && mongod --dbpath ./mongo/db  --fork --logpath ./mongo/mongod.log",
    "prestart:prod": "rimraf dist && tsc",
    "start:prod": "node dist/main.js",
    "start:hmr": "node dist/server",
    "lint": "tslint -p tsconfig.json -c tslint.json",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "webpack": "webpack --config webpack.config.js"
  },
  "dependencies": {
    "@nestjs/common": "^5.1.0",
    "@nestjs/core": "^5.1.0",
    "@nestjs/jwt": "^0.2.0",
    "@nestjs/mongoose": "^5.2.2",
    "@nestjs/passport": "^5.1.0",
    "cors": "^2.8.5",
    "dotenv": "^6.1.0",
    "google-auth-library": "^2.0.1",
    "googleapis": "^35.0.0",
    "joi": "^14.0.4",
    "jsonwebtoken": "^8.3.0",
    "jwt-decode": "^2.2.0",
    "mongoose": "^5.3.11",
    "passport": "^0.4.0",
    "passport-google-oauth20": "^1.0.0",
    "passport-http-bearer": "^1.0.1",
    "passport-jwt": "^4.0.0",
    "reflect-metadata": "^0.1.12",
    "rxjs": "^6.2.2",
    "typescript": "^3.0.1"
  },
  "devDependencies": {
    "@nestjs/testing": "^5.1.0",
    "@types/express": "^4.16.0",
    "@types/jest": "^23.3.1",
    "@types/joi": "^14.0.0",
    "@types/node": "^10.7.1",
    "@types/supertest": "^2.0.5",
    "jest": "^23.5.0",
    "nodemon": "^1.18.3",
    "prettier": "^1.14.2",
    "rimraf": "^2.6.2",
    "supertest": "^3.1.0",
    "ts-jest": "^23.1.3",
    "ts-loader": "^4.4.2",
    "ts-node": "^7.0.1",
    "tsconfig-paths": "^3.5.0",
    "tslint": "5.11.0",
    "webpack": "^4.16.5",
    "webpack-cli": "^3.1.0",
    "webpack-node-externals": "^1.7.2"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".spec.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
